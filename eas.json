{
  "cli": {
    "version": ">= 12.5.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "5c62xwqpqrfb",
        "EXPO_PUBLIC_API_URL": "https://zoom-backend-nu.vercel.app"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "5c62xwqpqrfb",
        "EXPO_PUBLIC_API_URL": "https://zoom-backend-nu.vercel.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {},
      "ios": {}
    }
  }
}

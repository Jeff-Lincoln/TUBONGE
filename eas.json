{
  "cli": {
    "version": ">= 12.5.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "d8dgdyfvmd57",
        "EXPO_PUBLIC_API_URL": "https://tubonge-backend-2.onrender.com"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "d8dgdyfvmd57",
        "EXPO_PUBLIC_API_URL": "https://tubonge-backend-2.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "d8dgdyfvmd57",
        "EXPO_PUBLIC_API_URL": "https://tubonge-backend-2.onrender.com"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_GET_STREAM_API_KEY": "d8dgdyfvmd57",
        "EXPO_PUBLIC_API_URL": "https://tubonge-backend-2.onrender.com"
      }
    }
  },
  "submit": {
    "production": {
      "android": {}
      // "ios": {}
    }
  }
}




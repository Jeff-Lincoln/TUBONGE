{
  "expo": {
    "name": "Tubonge",
    "slug": "tubonge1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Frame.png",
      "resizeMode": "contain",
      "backgroundColor": "##1a237e"
    },
    "ios": {
      "supportsTablet": true,
      "bitcode": false,
      "bundleIdentifier": "com.galaxies.firebase",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow this app to access your camera for live video calls.",
        "NSMicrophoneUsageDescription": "Allow this app to access your microphone for live audio calls."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "BLUETOOTH",
        "BLUETOOTH_CONNECT",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "MODIFY_AUDIO_SETTINGS",
        "SYSTEM_ALERT_WINDOW",
        "WAKE_LOCK"
      ],
      "package": "com.galaxies.firebase",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/Frame.png",
          "dark": {
            "image": "./assets/images/Frame.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          }
        }
      ],
      "@stream-io/video-react-native-sdk",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Camera",
            "Microphone"
          ]
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8af7063e-f76d-4cce-9dc7-6be5bda5b8f3"
      }
    },
    "owner": "lincoln_jeff"
  }
}
